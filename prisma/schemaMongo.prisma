generator client {
  provider = "prisma-client"
  output   = "../src/generated/prismaMongo"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGO")
}

model SensorType {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt   DateTime @db.Date
  description String
  name        String   @unique
}
